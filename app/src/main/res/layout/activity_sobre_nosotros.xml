<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.eniacs_team.rutamurcielago.Sobre_nosotros">

    <TextView
        android:id="@+id/info_general_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/info_aplicacion"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/desarrollado_por"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/desarrollado_por"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/info_general_app" />

    <TextView
        android:id="@+id/equipo_eniacs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/Eniacs"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/desarrollado_por" />

    <TextView
        android:id="@+id/integrantes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/integrantes"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/equipo_eniacs" />


    <TextView
        android:id="@+id/integrante1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/integrante1"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/integrantes"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/integrante2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/integrante2"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/integrante1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/integrante3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/integrante3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/integrante2"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/integrante4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/integrante4"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/integrante3"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/integrante5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/integrante5"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/integrante4"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/integrante6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/integrante6"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/integrante5"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/integrante7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/integrante7"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/integrante6"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/anno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/anno"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/integrante7" />






</android.support.constraint.ConstraintLayout>
